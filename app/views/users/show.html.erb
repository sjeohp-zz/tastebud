
<% if logged_in? %>
	
	
	
	
	<br />
	<%= form_for @tag, :url => {:controller => 'tags', :action => 'create'} do |form| %>
		<%= form.label :key %>
		<%= form.text_field :key %>
		<%= form.label :medium %>
		<%= form.text_field :medium %>
		<%= form.submit 'Add tag' %>
	<% end %>
	<br />
	<%= "#{@tagstring}" %>
<% end %>